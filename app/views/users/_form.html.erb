<%= form_for @user do |f| %>
    <% if @user.errors.any? %>
        <div id="errorExplanation">
          <h2><%= pluralize(@user.errors.count, "error") %> prohibited
            this user from being saved:</h2>
          <ul>
            <% @user.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <p>
      <%= f.label :Name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :Address %><br>
      <%= f.text_area :address %>
    </p>
    <p>
      <%= f.label :City %><br>
      <%= f.text_field :city %>
    </p>
    <p>
      <%= f.label :State %><br>
      <%= f.text_field :state %>
    </p>
    <p>
      <%= f.label :Zip_code %><br>
      <%= f.text_field :zip_code %>
    </p>
    <p>
      <%= f.label :School %><br>
      <%= f.text_field :school %>
    </p>
    <p>
      <%= f.label :Years_practice %><br>
      <%= f.text_field :years_practice %>
    </p>


    <p>
      <%= f.submit %>
    </p>
<% end %>
